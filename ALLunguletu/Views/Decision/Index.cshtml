@model IEnumerable<Lugo.EntityFramework.Decision>



<script>
    $(function() {
        $( "#accordion" ).accordion({
        collapsible: true
        });
    });
</script>

<h2>Lista decizii</h2>

@if(Model.Count() > 0)
{
<div id="accordion">
    @foreach(var item in Model)
    {
        <h3>@Html.DisplayFor(modelItem=>item.Data) - <b> @Html.DisplayFor(modelItem=>item.Subject) </b></h3>
        <div>
            <p>@Html.DisplayFor(modelItem=>item.Description) </p>
        </div>
    }
</div>
}

